@{
}
@model Common.Models.Report;

<form asp-action="banUser" asp-controller="BackPanel" enctype="multipart/form-data">

    <input type="text" name="userId" id="userId">

    <input type="submit" />
</form>

@{ foreach (var t in ViewBag.reports)
    {
    <div style="height= 200px; width=200px; background-color: yellow">
        <p>@t.creatorId.Name</p>
        <p>@t.reportComment</p>
        <p>type: @t.ReportType</p>
        <p>reason: @t.reportReason</p>
    </div>
    }
}
